        <title>Events by Severity</title>
        <searchString>[|inputlookup auditd_indicies] [|inputlookup auditd_sourcetypes] host=$host$ ($category$) ($severity$) ($type$) ($action$) (NOT user=* OR user=$user$) $other$ | timechart count by severity</searchString>
        <earliestTime>$time_selector.earliest$</earliestTime>
