      <chart>
        <title>Events by Host</title>
        <searchString>[|inputlookup auditd_indicies] [|inputlookup auditd_sourcetypes] host=$host$ ($category$) ($severity$) ($type$) ($action$) (NOT user=* OR user=$user$) $other$ | timechart count by host</searchString>
        <earliestTime>$time_selector.earliest$</earliestTime>
        <latestTime>$time_selector.latest$</latestTime>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">collapsed</option>
